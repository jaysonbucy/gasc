<div class="view-container">
  <% from = Date.new(2017, 8, 1) %>
  <% to = from + 6.months %>
  <table class="table table-striped table-responsive">
    <tr>
      <th class="meet-table">Meet Name</th>
      <th class="meet-table">Event</th>
      <th class="meet-table">Time</th>
    </tr>
    <% @presenter.swim_meet_times(from, to).each do |t| %>
    <tr>
      <td><%= t.event_heat.swim_meet.name %></td>
      <td><%= t.event_heat.swim_event.name %></td>
      <td><%= t.time_in_seconds %></td>
    </tr>
    <% end  %>
  </table>
</div>
